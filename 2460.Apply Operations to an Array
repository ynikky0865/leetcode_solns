class Solution:
    def applyOperations(self, nums: List[int]) -> List[int]:
        n = len(nums)
        for i in range(n-1):
            if  nums[i] == nums[i+1]:
                nums[i] *= 2
                nums[i+1] = 0
        res = [0] * n
        j = 0
        for ele in nums:
            if ele != 0:
                res[j] = ele
                j += 1
        return res
