class Solution:
    def summaryRanges(self, nums: List[int]) -> List[str]:
        res = []
        n = len(nums)
        start = 0
        for i in range(n):
            if i+ 1< n  and nums[i+1] == nums[i]+1:
                continue
            if start == i:
                res.append(str(nums[i]))
            else:
                res.append(str(nums[start])+"->"+str(nums[i]))
            start = i+1
        return res
