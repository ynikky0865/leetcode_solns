class Solution:
    def isZeroArray(self, nums: List[int], queries: List[List[int]]) -> bool:
        n = len(nums)
        pre_arr = [0] * (n+1)
        for left,right in queries:
            pre_arr[left] += 1
            pre_arr[right + 1] -= 1
        s = 0
        for i in range(len(nums)):
            s += pre_arr[i]
            if nums[i] > s:
                return False
        return True
