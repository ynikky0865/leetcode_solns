class Solution:
    def wordBreak(self, s: str, wordDict: List[str]) -> bool:
        n = len(s)
        res = [False] * (n+1)
        res[0] = True
        for i in range(1,n+1):
            for j in range(i):
                if res[j] and s[j:i] in wordDict :
                    res[i] = True
                    break
        return res[-1]
        
