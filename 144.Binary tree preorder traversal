class Solution:
    def preorderTraversal(self, root: Optional[TreeNode]) -> List[int]:
        if not root:
            return []
        res = []
        stack = []
        stack.append(root)
        while stack:
            ele = stack.pop()
            if ele:                
                res.append(ele.val)
            if ele.right:
                stack.append(ele.right)
            if ele.left:
                stack.append(ele.left)
        return res
