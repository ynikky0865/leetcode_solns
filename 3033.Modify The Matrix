class Solution:
    def modifiedMatrix(self, matrix: List[List[int]]) -> List[List[int]]:
        c=len(matrix[0])
        r=len(matrix)
        for j in range(c):
            mx=0
            for i in range(r):
                if matrix[i][j]>mx:
                    mx=matrix[i][j]
            for i in range(r):
                if matrix[i][j]==-1:
                    matrix[i][j]=mx
        return matrix
                
