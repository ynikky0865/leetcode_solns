class Solution:
    def removeKdigits(self, num: str, k: int) -> str:
        n = len(num)
        stack = []
        for ch in num:
            while stack and k > 0 and  stack[-1] > ch:
                stack.pop()
                k -= 1
            if stack or ch != '0':
                stack.append(ch)
        while stack and k > 0:
                stack.pop()
                k -= 1
        if not stack:
            return '0'
        return ''.join(stack)  
