class Solution:
    def maxProduct(self, words: List[str]) -> int:
        n=len(words)
        num_rep=[0]*n
        mx=0
        for i in range(n):
            for ch in words[i]:
                num_rep[i]|=1<<(ord(ch)-97)
            for j in range(i):
                if (num_rep[i]&num_rep[j])==0:
                    mx=max(mx,(len(words[i])*len(words[j])))
        return mx
