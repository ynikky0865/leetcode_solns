class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        n=len(nums)
        b=[1]*n
        a=[1]*n
        p=[0]*n
        for i in range(1,n):
            b[i]=b[i-1]*nums[i-1]
        for j in range(n-2,-1,-1):
            a[j]=a[j+1]*nums[j+1]
        for k in range(n):
            p[k]=a[k]*b[k]
        return p
