class Solution:
    def evalRPN(self, tokens: List[str]) -> int:
        st = []
        op = ['+','-','*','/']
        for i in tokens:
            if i not in op:
                st.append(int(i))
            else:
                b = st.pop()
                a = st.pop()
                if i == '+':
                    res = a + b
                elif i == '-':
                    res = a - b 
                elif i == '*':
                    res = a * b
                else:
                    res = int(a / b)
                st.append(res)
        return st[-1]       
